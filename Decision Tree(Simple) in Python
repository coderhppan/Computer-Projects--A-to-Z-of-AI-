ans1 = input("Has feathers? :")
if(ans1 == "yes"):
    ans2 = input("Can it fly? :")
    if(ans2 == "yes"):
        print("Your bird is Hawk")
 
    if(ans2 == "no"):
        print("Your bird is a Penguin")
 
if (ans1 == "no"):
    ans3 = input("Has fins? :")
 
    if(ans3 == "yes"):
       print("The fish is a dolphin")
 
    if(ans3 == "no"):
       print("The animal is a Bear")
